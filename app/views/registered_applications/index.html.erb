<div class="row">
  <div class="col-md-8">
    <h1>All Registered Applications</h1>
    <% @registered_applications.each do |registration| %>
      <p><%= link_to registration.name, registered_application_path(registration.id) %></p>
    <% end %>
  </div>
  <div class="col-md-4">
    <%= link_to "Register Applications", new_registered_application_path, :class => 'btn btn-primary'%>
  </div>
</div>
